{
  "manifest_version": 2,
  "name": "Aviasales checker",
  "description": "This extension is need to help us to find the cheapest flies over the world via site aviasales.ru",
  "version": "0.1",
  "background": {
    "scripts": ["background.js", "plugins/moment.js"],
    "persistent": false
  },
  "icons": {
    "16": "48x48.png",
    "32": "48x48.png",
    "48": "48x48.png",
    "128": "48x48.png"
  },
  "permissions": [
    "tabs",
//    "activeTab",
//    "contextMenus",
    "storage",
//    "bookmarks",
    "alarms"
  ],
  "browser_action": {
    "default_title": "Открыть aviasales checker",
    "default_icon": "48x48.png",
    "default_popup": "popup.html"
  }
}
